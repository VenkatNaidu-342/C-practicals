#include<stdio.h>
int main()
{
    int i=1,n,k=0;
    printf("Enter any number: ");
    scanf("%d",&n);
    while(i<=n)
    {
        if(n%i==0)
        {
            k = k + 1;
        }
        i++;
    }
    if(k == 2)
    {
        printf("PRIME NUMBER");
    }
    else
    {
        printf("NOT PRIME NUMBER");
    }
    return 0;
}
